<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>

    <package name="leaderboard" extends="base" namespace="/leaderboard">

        <action name="challenger" class="leaderboard.action.BoardAction" method="getChallengerBoard">
            <result name="success">leaguelist.jsp</result>
        </action>
        <action name="master" class="leaderboard.action.BoardAction" method="getMasterBoard">
            <result name="success">leaguelist.jsp</result>
        </action>
    </package>

</struts>